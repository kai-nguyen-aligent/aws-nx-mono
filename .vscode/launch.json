{
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "hello-sam:src/lambda/hello.lambdaHandler (nodejs18.x)",
            "invokeTarget": {
                "target": "code",
                "projectRoot": "${workspaceFolder}",
                "lambdaHandler": "services/hello-sam/src/lambda/hello.lambdaHandler"
            },
            "lambda": {
                "runtime": "nodejs18.x",
                "payload": {
                    "json": {
                        "body": "test"
                    }
                },
                "environmentVariables": {}
            }
        }
    ]
}
